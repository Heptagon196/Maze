# Default map
##include include/menu.gos
##include include/backpack.gos
bag := backpack;
NewWall := 0;
defBlock(NewWall, WHITE, BLUE, "  ", true);
setBlock(NewWall, "maze.map");
details := list();
setKeyEvent("e", func() {
    i := bag.chooseItem();
    if (i != -1 && i < size(details)) {
        say(details[i]);
    }
});
##include maps/Default/events1.gos
##include maps/Default/events2.gos
mapInit();
